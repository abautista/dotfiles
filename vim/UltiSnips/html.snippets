# META TAGS 
snippet metaviewport "Viewport" !b
<meta name="viewport" content="width=device-width, initial-scale=1">
endsnippet

# Attributes
snippet .
class="${1}"
endsnippet

snippet #
id="${1}"
endsnippet

snippet href
href="${1:#}"
endsnippet

snippet data 
data=${1}="${2:$1}"
endsnippet

# Elements 
snippet a
<a href="${1:#}">${0:$1}</a>
endsnippet

snippet a.
<a class="${1}" href="${2:#}">${0:$1}</a>
endsnippet

snippet a#
<a id="${1}" href="${2:#}">${0:$1}</a>
endsnippet

snippet div
<div>
  ${0}
</div>
endsnippet

snippet div.
<div class="${1}">
  ${0}
</div>
endsnippet

snippet div#
<div id="${1}">
  ${0}
</div>
endsnippet

snippet dt
