[user]
	name = Alberto Bautista
	email = alberto.bautista@me.com
	username = abautista

[core]
	excludesfile = /Users/abautista/.gitignore_global
	editor = vim

[alias]
  l = log --oneline --abbrev-commit --abbrev=4 -25
  s = status -s

# Any GitHub repo with my username should be checked out r/w by default
# http://rentzsch.tumblr.com/post/564806957/public-but-hackable-git-submodules
[url "git@github.com:abautista/"]
	insteadOf = "git://github.com/abautista/"
